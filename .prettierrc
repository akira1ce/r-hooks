{
	"semi": true,
	"trailingComma": "es5",
	"printWidth": 120,
	"tabWidth": 2,
	"useTabs": true,
	"endOfLine": "lf",
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": ["^react$", "^react-dom$", "^react/(.*)$", "^@/(.*)$", "^[./]"],
	"importOrderSortSpecifiers": true,
	"importOrderGroupNamespaceSpecifiers": true,
	"importOrderCaseInsensitive": true
}
